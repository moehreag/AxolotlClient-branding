{
	"schemaVersion": 1,
	"id": "axolotlclient",
	"version": "${version}",
	"name": "AxolotlClient",
	"description": "Free & Open-Source Minecraft Mod for use in PvP and other Scenarios",
	"authors": [
		"moehreag"
	],
	"contributors": [
		"YakisikliBaran",
		"DragonEggBedrockBreaking",
		"TheKodeToad",
		"CornetPanique86"
	],
	"contact": {
		"homepage": "https://axolotlclient.github.io/",
		"sources": "https://github.com/AxolotlClient/AxolotlClient-mod/",
		"issues": "https://github.com/AxolotlClient/AxolotlClient-mod/issues"
	},
	"license": "LGPL-3.0-or-later",
	"icon": "assets/axolotlclient/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"io.github.axolotlclient.AxolotlClient"
		],
		"modmenu": [
			"io.github.axolotlclient.config.modmenu.ModMenuCompat"
		]
	},
	"mixins": [
		"axolotlclient.mixins.json"
	],
	"custom": {
		"modmenu:clientsideOnly": true
	},
	"depends": {
		"fabricloader": ">=0.15.0",
		"minecraft": [
			"1.16.3-combat.8.c",
			"1.16.3-combat.8.a"
		],
		"fabric-resource-loader-v0": "*",
		"axolotlclientconfig": "*",
		"axolotlclient-common": "*"
	}
}
